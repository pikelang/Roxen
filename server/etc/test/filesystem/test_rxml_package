<define container="italic"><i>&_.contents;</i></define>

<define tag="bä"><attrib name="x">Mu</attrib>&_.x;!</define>
<define tag="gazonx">bar</define>

<define variable="var.a">AA</define>
<set variable="var.b" value="BB"/>

<define if="rxml_test">xyz</define>
<define if="rxml_test2">abc</define>

<undefine container="italic"/>
